<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tmizzle2005.myriadChallenge" >

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tmizzle2005.myriadChallenge.activity.SignUp"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tmizzle2005.myriadChallenge.activity.KingDomList"></activity>

        <activity
            android:name="com.example.tmizzle2005.myriadChallenge.activity.KingdomInfo"></activity>

        <activity
            android:name="com.example.tmizzle2005.myriadChallenge.activity.Progress"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="com.example.tmizzle2005.myriadChallenge.activity.Nicer_Quest"></activity>
    </application>

</manifest>
